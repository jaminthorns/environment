{
  "Print a string": {
    "prefix": "put",
    "body": ["IO.puts(\"$1\")"]
  },
  "Inspect a value": {
    "prefix": "ins",
    "body": ["IO.inspect($1, IEx.inspect_opts() ++ [label: \"$2\"])"]
  },
  "Inspect a value (piped)": {
    "prefix": "ins",
    "body": ["IO.inspect(IEx.inspect_opts() ++ [label: \"$1\"])"]
  },
  "Debug some code": {
    "prefix": "pry",
    "body": ["require IEx\nIEx.pry()"]
  }
}
