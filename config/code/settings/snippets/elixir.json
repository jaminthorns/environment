{
  "put": {
    "prefix": "put",
    "body": ["IO.puts(\"$1\")"],
    "description": "Print a string"
  },
  "inspect": {
    "prefix": "ins",
    "body": ["IO.inspect($1, label: \"$1\")"],
    "description": "Inspect a value"
  },
  "inspect_piped": {
    "prefix": "ins",
    "body": ["IO.inspect(label: \"$1\")"],
    "description": "Inspect a value (piped)"
  }
}
