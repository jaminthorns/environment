{
  "Print a string": {
    "prefix": "put",
    "body": ["IO.puts(\"$1\")"]
  },
  "Inspect a value": {
    "prefix": "ins",
    "body": ["IO.inspect($1, label: \"$2\")"]
  },
  "Inspect a value (piped)": {
    "prefix": "ins",
    "body": ["IO.inspect(label: \"$1\")"]
  },
  "Debug some code": {
    "prefix": "pry",
    "body": ["require IEx\nIEx.pry()"]
  }
}
