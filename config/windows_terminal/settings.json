{
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "defaultProfile": "{2c4de342-38b7-51cf-b940-2309a097f518}",
  "tabSwitcherMode": "disabled",
  "profiles": {
    "defaults": {
      "colorScheme": "Dracula",
      "cursorShape": "filledBox",
      "font": {
        "size": 8,
        "face": "JetBrainsMono Nerd Font Mono"
      },
      "antialiasingMode": "cleartype",
      "intenseTextStyle": "bold",
      "bellStyle": "none"
    },
    "list": [
      {
        "guid": "{2c4de342-38b7-51cf-b940-2309a097f518}",
        "name": "Linux",
        "commandline": "wsl.exe ~"
      },
      {
        "guid": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
        "name": "PowerShell",
        "commandline": "powershell.exe"
      },
      {
        "guid": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
        "name": "Command Prompt",
        "commandline": "cmd.exe"
      }
    ]
  },
  "schemes": [
    {
      "name": "Dracula",
      "cursorColor": "#F8F8F2",
      "selectionBackground": "#44475A",
      "background": "#282A36",
      "foreground": "#F8F8F2",
      "black": "#21222C",
      "blue": "#BD93F9",
      "cyan": "#8BE9FD",
      "green": "#50FA7B",
      "purple": "#FF79C6",
      "red": "#FF5555",
      "white": "#F8F8F2",
      "yellow": "#F1FA8C",
      "brightBlack": "#6272A4",
      "brightBlue": "#D6ACFF",
      "brightCyan": "#A4FFFF",
      "brightGreen": "#69FF94",
      "brightPurple": "#FF92DF",
      "brightRed": "#FF6E6E",
      "brightWhite": "#FFFFFF",
      "brightYellow": "#FFFFA5"
    }
  ],
  "keybindings": [
    {
      "command": "copy",
      "keys": "ctrl+c"
    },
    {
      "command": "paste",
      "keys": "ctrl+v"
    },
    {
      "command": "find",
      "keys": "ctrl+f"
    },
    {
      "command": "newTab",
      "keys": "ctrl+shift+n"
    },
    {
      "command": "prevTab",
      "keys": "ctrl+shift+left"
    },
    {
      "command": "nextTab",
      "keys": "ctrl+shift+right"
    },
    {
      "command": { "action": "splitPane", "split": "vertical" },
      "keys": "ctrl+shift+\\"
    },
    {
      "command": { "action": "moveFocus", "direction": "left" },
      "keys": "ctrl+shift+["
    },
    {
      "command": { "action": "moveFocus", "direction": "right" },
      "keys": "ctrl+shift+]"
    },
    {
      "command": { "action": "clearBuffer", "clear": "all" },
      "keys": "ctrl+k"
    },
    {
      "command": null,
      "keys": "alt+enter"
    }
  ]
}
